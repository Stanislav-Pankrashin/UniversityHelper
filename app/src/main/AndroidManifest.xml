<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.self_employed.stase.universityhelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/uh_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home_Screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Semester_Gpa"
            android:parentActivityName=".Home_Screen"></activity>
        <activity
            android:name=".PaperGpaCalc"
            android:parentActivityName=".Home_Screen"></activity>
    </application>

</manifest>